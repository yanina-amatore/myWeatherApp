{
  "name": "react-boilerplate",
  "version": "0.2.0",
  "description": "My WeatherApp",
  "repository": "https://github.com/dev-academy-challenges/react-boilerplate.git",
  "main": "server/index.js",
  "scripts": {
    "start": "run-p webpack server",
    "dev": "run-p client:watch server:watch",
    "webpack": "webpack --config client/webpack.config.js",
    "server": "node server",
    "client:watch": "npm run webpack -- --watch",
    "server:watch": "nodemon server",
    "test": "jest --watchAll --verbose",
    "lint": "eslint --ext .js,.jsx ."
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nock": "^13.2.9",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "bootstrap-icons": "^1.10.2",
    "css-loader": "^6.7.2",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.17.2",
    "react-select": "^5.5.9",
    "react-select-async-paginate": "^0.7.0",
    "superagent": "7.1.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "browsers": [
        "chrome",
        "firefox",
        "safari"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
